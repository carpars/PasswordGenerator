{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WebConfig": {
    "Password": {
      "Length": 128,
      "Settings": {
        "SavePreferences": true,
        "HidePassword": true,
        "IncludeNumbers": true,
        "IncludeLowercase": true,
        "IncludeUppercase": true,
        "AutoCopyToClipboard": true
      }
    }
  }
}